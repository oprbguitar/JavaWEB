<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Ejemplo1</servlet-name>
        <servlet-class>Aplicaciones.Ejemplo1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ejemplo2</servlet-name>
        <servlet-class>Aplicaciones.Ejemplo2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ejemplo3</servlet-name>
        <servlet-class>Aplicaciones.Ejemplo3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ejemplo4</servlet-name>
        <servlet-class>Aplicaciones.Ejemplo4</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ejemplo1</servlet-name>
        <url-pattern>/Ejemplo1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ejemplo2</servlet-name>
        <url-pattern>/Ejemplo2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ejemplo3</servlet-name>
        <url-pattern>/Ejemplo3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ejemplo4</servlet-name>
        <url-pattern>/Ejemplo4</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
